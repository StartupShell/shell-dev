<head>
	<meta charset="utf-8">
	<meta http-equiv="X-UA-Compatible" content="IE=edge,chrome=1">
	<title>Startup Shell</title>
	<meta name="description" content="">
    <meta name="viewport" content="width=device-width">

    <!-- Facebook Open Graph meta tags -->
        <meta property="og:image" content="http://i.imgur.com/e1dNQDT.png" />
        <meta property="og:title" content="Startup Shell" />
        <meta property="og:url" content="http://startupshell.org" />
        <meta property="og:site_name" content="Startup Shell" />
        <meta property="og:type" content="website" />
        <meta property="og:description" content="Startup Shell is a student-run technology collective that advances ventures while leading educational initiatives in our greater community. We value students, technology, entrepreneurship and innovation, and collaboration. The Shell provides hackerspace resources and business direction through a lively coworking environment in our space on the University of Maryland campus." />
        <!-- <meta property="fb:app_id" content="179294755555664" /> -->
    <!-- end Facebook Open Graph meta tags --> 

	<%= stylesheet_link_tag "normalize.min", "style" %>
	<%= javascript_include_tag "vendor/modernizr-2.6.2.min" %>

	<%= csrf_meta_tags %>
    <script type="application/javascript">
  (function(i,s,o,g,r,a,m){i['GoogleAnalyticsObject']=r;i[r]=i[r]||function(){
  (i[r].q=i[r].q||[]).push(arguments)},i[r].l=1*new Date();a=s.createElement(o),
  m=s.getElementsByTagName(o)[0];a.async=1;a.src=g;m.parentNode.insertBefore(a,m)
  })(window,document,'script','//www.google-analytics.com/analytics.js','ga');

  ga('create', 'UA-43752415-1', 'startupshell.org');
  ga('send', 'pageview');

</script>
